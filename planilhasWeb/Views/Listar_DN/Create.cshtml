                                    <div class="col-sm-12">
                                        <div class="col-sm-2" style="clear:both;">
                                            <div class="form-group">
                                                <label>1º Parcela</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">R$</span>
                                                    @Html.TextBoxFor(x => x.parcela_1, new { @class = "form-control", id = "parcela_1", @placeholder = "parcela_1", Value = String.Format("{0:n2}", Model.parcela_1), @style = "background-color:#DEE2EB;" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-2">
                                            <div class="form-group">
                                                <label>Lance</label>
                                                <div class="input-group" style="margin-bottom:0.4px;">
                                                    @Html.TextBoxFor(x => x.lance, new { @class = "form-control", id = "lance", @placeholder = "lance", Value = String.Format("{0:n0}", Model.lance), @style = "background-color:#DEE2EB;" })
                                                    <span class="input-group-addon" style="padding-left:19px;">% </span>
                                                </div>
                                                <div class="input-group">
                                                    <span class="input-group-addon">R$</span>
                                                    @Html.TextBoxFor(x => x.lance_valor, new { @class = "form-control", id = "lance_valor", @placeholder = "lance_valor", @style = "border-top:none;", Value = String.Format("{0:n2}", Model.lance_valor) })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-2">
                                            <div class="form-group">
                                                <label>Saldo Devedor</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon">R$</span>
                                                    @Html.TextBoxFor(x => x.saldo_devedor, new { @class = "form-control", id = "saldo_devedor", @placeholder = "saldo_devedor", Value = String.Format("{0:n2}", Model.saldo_devedor) })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-2">
                                            <div class="form-group">
                                                <div class="form-group">
                                                    <label>Rateio Meses -1ª</label>
                                                    <div class="input-group">
                                                        <span class="input-group-addon">R$</span>
                                                        @Html.TextBoxFor(model => model._qte_meses, new { @class = "form-control", @placeholder = "_qte_meses", Value = String.Format("{0:n0}", Model._qte_meses) })
                                                        @Html.TextBoxFor(model => model.valor_qtd_meses, new { @class = "form-control", @placeholder = "valor_qtd_meses", Value = String.Format("{0:n2}", Model.valor_qtd_meses) })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-2">
                                            <div class="form-group">
                                                <div class="form-group">
                                                    <label>Sem diluir</label>
                                                    <div class="input-group">

                                                        @Html.TextBoxFor(model => model._j38, new { @class = "form-control", @placeholder = "_j38", Value = String.Format("{0:n1}", Model._j38) })
                                                        <span class="input-group-addon">Meses</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-2">
                                            <div class="form-group">
                                                <div class="form-group">
                                                    <label>Desde a 1ª Parc.</label>
                                                    <div class="input-group">
                                                        <span class="input-group-addon">R$</span>
                                                        @Html.TextBoxFor(model => model._l39, new { @class = "form-control", @placeholder = "_l39", Value = String.Format("{0:n2}", Model._l39) })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="col-sm-10">
                                        <span class="noPrint">
                                            <input type="submit" value="Salvar" class="btn btn-default col-sm-1" />
                                        </span>
                                        @*<input type="reset" value="Limpar" class="btn btn-default col-sm-1" onclick="limpaForm()" />*@
                                        @*@Html.ActionLink("Criar nova", "Create")*@
                                        <br />

                                    </div>

                                </form>


                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-sm-12 -->
            </div>

                <!-- /.row -->
            <!--</div>  -->
        <!-- /#page-wrapper -->
        }
